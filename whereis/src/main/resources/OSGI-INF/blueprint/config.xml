<?xml version="1.0" encoding="UTF-8"?>
<blueprint xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xmlns:jpa="http://aries.apache.org/xmlns/jpa/v2.0.0"
           xmlns="http://www.osgi.org/xmlns/blueprint/v1.0.0"
           xsi:schemaLocation="http://www.osgi.org/xmlns/blueprint/v1.0.0 http://www.osgi.org/xmlns/blueprint/v1.0.0/blueprint.xsd">
  <jpa:enable/>
  <reference id="logService" interface="org.osgi.service.log.LogService" availability="optional"/>
  <service ref="WhereIsKeyword">
    <interfaces>
      <value>au.id.vanlaatum.botter.api.KeyWordProcessor</value>
      <value>org.osgi.service.cm.ManagedService</value>
    </interfaces>
    <service-properties>
      <entry key="metatype.pid" value="au.id.vanlaatum.botter.connector.whereis"/>
      <entry key="service.pid" value="au.id.vanlaatum.botter.connector.whereis"/>
    </service-properties>
  </service>
</blueprint>
